{
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "black-formatter.args": [
        "--line-length",
        "98"
    ],
    "python.analysis.extraPaths": [
        "EAPM/deps/",
        "EAPM/Include/",
    ],
    "python.envFile": "${workspaceFolder}/.env",
    "esbonio.sphinx.confDir": "",
    "cSpell.words": [
        "ahatool",
        "Asitedesign",
        "bsceapm",
        "eapm",
        "hmmer",
        "mafft"
    ]
}