{
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "black-formatter.args": [
        "--line-length",
        "98"
    ],
    "python.analysis.extraPaths": [
        "EAPM/deps/",
        "EAPM/Include/",
    ],
    "python.analysis.typeCheckingMode": "basic",
    "pylint.args": [
        "--rcfile=${workspaceFolder}/.pylintrc"
    ],
    "css.customData": [
        ".vscode/tailwind.json"
    ],
}