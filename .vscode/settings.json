{
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "black-formatter.args": [
        "--line-length",
        "98"
    ],
    "python.analysis.typeCheckingMode": "off",
    "python.analysis.extraPaths": [
        "EAPM/deps/",
        "EAPM/Include/",
    ],
    "python.envFile": "${workspaceFolder}/.env",
    "esbonio.sphinx.confDir": ""
}
